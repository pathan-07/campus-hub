{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "redirects": [
    {
      "source": "/auth/callback/",
      "destination": "/auth/callback",
      "permanent": false
    },
    {
      "source": "/",
      "has": [{ "type": "query", "key": "code" }],
      "destination": "/auth/callback",
      "permanent": false
    }
  ]
}